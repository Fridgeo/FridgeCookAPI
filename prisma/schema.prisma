generator client {
  provider     = "prisma-client"
  output       = "../generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model cache_recettes {
  id          Int       @id @default(autoincrement())
  search_key  Json
  recipe_data Json
  created_at  DateTime? @default(now()) @db.Timestamp(6)

  @@index([search_key], map: "idx_search_key", type: Gin)
}
